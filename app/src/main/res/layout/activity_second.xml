<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
android:background="@drawable/arkaplan"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity">

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtkuvvet"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:enabled="false"
        android:autoText="true"
        android:textIsSelectable="false"
        android:inputType="none"
        android:textAlignment="center"
        android:textColor="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Girilen Kuvvet Değeri "
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="#ffff" />

    <RelativeLayout tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_below="@+id/edtkuvvet"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp"
        android:id="@+id/relativeLayout">

    </RelativeLayout>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtegilme"
        android:layout_below="@+id/relativeLayout2"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:textAlignment="center"
        android:textColor="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eğilme Gerilimi"
        android:id="@+id/textView3"
        android:layout_below="@+id/edtcekme"
        android:layout_alignStart="@+id/edtegilme"
        android:layout_marginTop="39dp"
        android:textColor="#ffff" />

    <RelativeLayout tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_below="@+id/textView3"
        android:layout_alignLeft="@+id/relativeLayout"
        android:id="@+id/relativeLayout2">

    </RelativeLayout>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtkayma"
        android:layout_below="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:enabled="true"
        android:textAlignment="center"
        android:textColor="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kayma Gerilimi"
        android:id="@+id/textView5"
        android:layout_below="@+id/edtegilme"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="48dp"
        android:textColor="#ffff" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtcekme"
        android:layout_alignTop="@+id/relativeLayout"
        android:layout_alignEnd="@+id/textView3"
        android:enabled="true"
        android:textAlignment="center"
        android:textColor="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Çekme Gerilimi"
        android:id="@+id/textView2"
        android:layout_above="@+id/edtcekme"
        android:layout_alignStart="@+id/edtkuvvet"
        android:textColor="#ffff" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtemniyet"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:enabled="true"
        android:textAlignment="center"
        android:textColor="#ffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emniyet Kontrolü"
        android:id="@+id/textView6"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/edtkayma"
        android:layout_alignStart="@+id/textView5"
        android:textColor="#ffff" />

</RelativeLayout>
