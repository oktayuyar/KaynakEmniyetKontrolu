<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="16dp"
    tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity">

    <RelativeLayout tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="43dp"
        android:id="@+id/relativeLayout">

    </RelativeLayout>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtegilme"
        android:enabled="true"
        android:textAlignment="center"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eğilme Gerilimi"
        android:id="@+id/textView3"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/edtcekme"
        android:layout_alignEnd="@+id/relativeLayout2" />

    <RelativeLayout tools:context="com.example.oktay.kaynakemniyetkontrolu.SecondActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_below="@+id/textView3"
        android:layout_alignLeft="@+id/relativeLayout"
        android:id="@+id/relativeLayout2">

    </RelativeLayout>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtkayma"
        android:layout_below="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:enabled="true"
        android:textAlignment="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kayma Gerilimi"
        android:id="@+id/textView5"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/edtegilme" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtcekme"
        android:enabled="true"
        android:textAlignment="center"
        android:layout_alignTop="@+id/relativeLayout"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Çekme Gerilimi"
        android:id="@+id/textView2"
        android:layout_above="@+id/edtcekme"
        android:layout_alignEnd="@+id/relativeLayout" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/edtemniyet"
        android:enabled="true"
        android:textAlignment="center"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/edtkayma" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emniyet Kontrolü"
        android:id="@+id/textView6"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/edtkayma"
        android:layout_alignStart="@+id/textView5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="İncele"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignStart="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="İncele"
        android:id="@+id/button3"
        android:layout_marginEnd="40dp"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="İncele"
        android:id="@+id/button4"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignStart="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="incele"
        android:id="@+id/button5"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignStart="@+id/button4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/txtsonuc"
        android:layout_below="@+id/edtemniyet"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center" />

</RelativeLayout>
